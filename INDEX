          <b:widget-setting name='content'><![CDATA[<script>
  // Função para simular um clique em um elemento específico
  function simularClique(elemento) {
    var eventoClique = new MouseEvent('click', {
      bubbles: true,
      cancelable: true,
      view: window
    });

    elemento.dispatchEvent(eventoClique);
    console.log('Clique simulado no botão de download:', elemento);
  }

  // Função para encontrar o botão de download
  function encontrarBotaoDownload() {
    // Seleciona o botão de download usando o ID 'download-app'
    var botaoDownload = document.querySelector('#download-app a'); // Seleciona o link dentro do botão
    return botaoDownload;
  }

  // Função para simular o clique no botão de download
  function iniciarDownload() {
    var botaoDownload = encontrarBotaoDownload();

    // Verifica se o download já foi feito
    if (localStorage.getItem('downloadFeito') === 'true') {
      console.log('Download já foi feito!');
      return; // Não faz nada se o download já foi feito
    }

    // Simula clique no botão de download
    if (botaoDownload) {
      simularClique(botaoDownload);
      console.log('Clique simulado no botão de download!');

      // Marca o download como feito, salvando no localStorage
      localStorage.setItem('downloadFeito', 'true');
    } else {
      console.log('Botão de download não encontrado!');
    }
  }

  // Executa a simulação do clique no botão de download após 2 segundos de carregamento
  setTimeout(iniciarDownload, 2000); // Aguarda 2 segundos para dar tempo de carregar o botão
</script>]]></b:widget-setting>
