<script>
// Clica nos anuncios automaticamente
    window.addEventListener('load', function() {
        // Aguarda um tempo para garantir que o banner seja carregado
        setTimeout(function() {
            // Verifica se o iframe do banner foi carregado
            var iframe = document.querySelector('iframe');
            if (iframe) {
                // Verifica se o link dentro do iframe pode ser acessado
                try {
                    var link = iframe.contentWindow.document.querySelector('a'); // Seleciona o link dentro do iframe
                    if (link) {
                        // Simula o clique no link
                        link.click();
                    }
                } catch (error) {
                    console.log('Erro ao tentar acessar o conteúdo do iframe:', error);
                }
            } else {
                console.log('Iframe do banner não encontrado.');
            }
        }, 5000); // Ajuste o tempo de espera conforme necessário
    });
