<!-- Script -->
<script>
    // Mostrar o banner pop-up
    function mostrarBanner() {
        document.getElementById('banner-popup').style.display = 'flex';
    }

    // Fechar o banner pop-up
    function fecharBanner() {
        document.getElementById('banner-popup').style.display = 'none';
    }

    // Cronômetro
    function iniciarCronometro() {
        const cronometro = document.getElementById('cronometro');
        const agora = new Date().getTime();

        let tempoFinal = localStorage.getItem('tempoFinal');
        if (!tempoFinal || agora >= tempoFinal) {
            const minutosAleatorios = Math.floor(Math.random() * (15 - 10 + 1)) + 10;
            tempoFinal = agora + minutosAleatorios * 60 * 1000;
            localStorage.setItem('tempoFinal', tempoFinal);
        }

        function atualizarCronometro() {
            const agora = new Date().getTime();
            const tempoRestante = Math.max(tempoFinal - agora, 0);

            if (tempoRestante > 0) {
                const minutos = Math.floor((tempoRestante / 1000) / 60);
                const segundos = Math.floor((tempoRestante / 1000) % 60);
                cronometro.textContent = `${minutos.toString().padStart(2, '0')}:${segundos.toString().padStart(2, '0')}`;
            } else {
                const minutosAleatorios = Math.floor(Math.random() * (15 - 10 + 1)) + 10;
                tempoFinal = agora + minutosAleatorios * 60 * 1000;
                localStorage.setItem('tempoFinal', tempoFinal);
            }
        }

        setInterval(atualizarCronometro, 1000);
        atualizarCronometro();
    }

    window.onload = iniciarCronometro;
</script>]]></b:widget-setting>
        </b:widget-settings>
