<!-- Linkagem Interna Automática Melhorada -->
<script>
document.addEventListener("DOMContentLoaded", function() {
  let palavrasChave = {
    "palpites": "https://www.acertosclub.com.br/palpites",
    "jogos": "https://www.acertosclub.com.br/jogos",
    "cassino": "https://www.acertosclub.com.br/cassino",
    "loterias": "https://www.acertosclub.com.br/loterias",
    "apostas": "https://www.acertosclub.com.br/apostas"
  };

  document.querySelectorAll("article p").forEach(paragrafo => {
    let texto = paragrafo.innerHTML;
    Object.keys(palavrasChave).forEach(palavra => {
      let regex = new RegExp(`\\b(${palavra})\\b`, "gi");
      let link = `<a href="${palavrasChave[palavra]}" title="${palavra}">${palavra}</a>`;

      // Verifica se a palavra já está dentro de um link para evitar duplicações
      let tempDiv = document.createElement("div");
      tempDiv.innerHTML = texto;
      let palavrasExistentes = tempDiv.querySelectorAll("a");

      let jaLinkado = Array.from(palavrasExistentes).some(a => a.innerText.toLowerCase() === palavra.toLowerCase());

      if (!jaLinkado) {
        texto = texto.replace(regex, link);
      }
    });
    paragrafo.innerHTML = texto;
  });
});
</script>]]></b:widget-setting>
