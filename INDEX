<script>
    function toggleSidebar() {
        var sidebar = document.getElementById(&#39;sidebar&#39;);
        if (sidebar.classList.contains(&#39;active&#39;)) {
            sidebar.classList.remove(&#39;active&#39;);
        } else {
            sidebar.classList.add(&#39;active&#39;);
        }
    }
</script>
